{
  "name": "inventory-management-system",
  "version": "1.0.0",
  "author": "Rafa≈Ç Chmielewski <rafalchmielewski.praca@gmail.com>",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node ./src/app",
    "start:debug": "node --nolazy --inspect=64009 -r ts-node/register ./src/app",
    "test": "ts-mocha $(find . -path ./node_modules -prune -o -name '*.spec.ts') --reporter mocha-multi-reporters --reporter-options configFile=src/test/reporter_config.json  --timeout 20000 --exit",
    "test:coverage": "nyc --reporter=text-summary --reporter=text --reporter=lcov npm run test",
    "prettier": "prettier --write \"src/**/*.{ts,js}?(x)\"",
    "lint": "eslint --ext 'ts' .",
    "lint:fix": "eslint --ext 'ts' --fix ."
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "dotenv": "16.4.5",
    "express": "4.18.0",
    "express-validator": "6.14.0",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mongoose": "6.6.5",
    "morgan": "1.10.0",
    "passport": "0.5.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "6.3.1",
    "@types/chai": "4.3.1",
    "@types/chai-spies": "1.0.3",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lodash": "4.14.182",
    "@types/mocha": "10.0.0",
    "@types/morgan": "1.9.3",
    "@types/node": "18.11.9",
    "@types/passport": "1.0.7",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "chai-spies": "1.0.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "mocha": "10.1.0",
    "mocha-multi-reporters": "1.5.1",
    "mocha-sonarqube-reporter": "1.0.2",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "sinon": "14.0.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  }
}
